    "    elif (time_range==\"день\"):\n",
    "        start_date = end_date+timedelta(days=-1)\n",
    "        data_user=quotes_hour[quotes_hour[\"Date\"]>=start_date]\n",
    "    elif (time_range==\"дней\"):\n",
    "        start_date = end_date+timedelta(days=-7)\n",
    "        data_user=quotes_hour[quotes_hour[\"Date\"]>=start_date]\n",
    "    elif (time_range==\"месяц\"):\n",
    "        start_date = end_date+timedelta(days=-31)\n",
    "        data_user=quotes_day[quotes_day[\"Date\"]>=start_date]\n",
    "    elif (time_range==\"месяцев\"):\n",
