    "def fundamental_analysis(mult, companies_choice):\n",
    "    curr_mult=[]\n",
    "    if mult=='EV/EBITDA':\n",
    "        curr_mult=ev_ebitda\n",
    "    elif mult=='P/E':\n",
    "        curr_mult=p_e\n",
    "    elif mult=='EPS':\n",
    "        curr_mult=eps\n",
    "    elif mult=='DY':\n",
    "        curr_mult=div_yield\n",
    "    elif mult=='DPR':\n",
    "        curr_mult=div_payout_ratio\n",
    "    elif mult=='DEBT/EBITDA':\n",
    "        curr_mult=debt_ebitda\n",
    "    elif mult=='NET DEBT':\n",
    "        curr_mult=net_debt\n",
    "    elif mult=='ROA':\n",
    "        curr_mult=roa\n",
    "    elif mult=='ROE':\n",
    "        curr_mult=roe\n",
    "\n",
    "    fig = go.Figure()\n",
    "    fig.update_layout(title=f\"Анализ мультипликатора {mult}\", xaxis_title=\"Года\", yaxis_title=\"Значение мультипликатора\")\n",
    "    for comp in companies_choice:\n",
    "        fig.add_trace(go.Scatter(x=curr_mult.loc[comp].index, y=curr_mult.loc[comp].values, name=comp))\n",
    "    filename=f'{mult}.png'\n",
    "    fig.write_image(filename)\n",
    "    PATH=filename\n",
    "    im=pyimgur.Imgur(CLIENT_ID)\n",
    "    uploaded_image=im.upload_image(PATH, title=PATH)\n",
    "    return uploaded_image"
   ]
