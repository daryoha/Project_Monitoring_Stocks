    "def technical_analysis(company, pattern, indicator, oscillator):\n",
    "    fig = make_subplots(rows=2, cols=1, shared_xaxes=True, vertical_spacing=0.03,\n",
    "            subplot_titles=('GAZP_price', 'Indicator'), row_width=[0.2, 0.7])\n",
    "\n",
    "    candle_graph = go.Candlestick(x=df_tech['begin'], open=df_tech[f'open_{company}'], high=df_tech[f'high_{company}'],\n",
    "                                  low=df_tech[f'low_{company}'], close=df_tech[f'close_{company}'])\n",
    "    fig.add_trace(candle_graph, row = 1, col = 1) #отрисовываем основной график\n",
    "\n",
    "#Рисуем паттерны -------------------------------------------------------------------------------------------------------\n",
    "    if pattern=='3 Line Strike':\n",
    "        Line_Strike = df_tech[df_tech[f'3_Line_Strike_{company}']!=0]\n",
    "        fig.add_trace(go.Scatter(x=Line_Strike['begin'], y=Line_Strike[f'high_{company}'], mode='markers',\n",
    "                    name='3_Line_Strike', marker=dict(symbol='circle', size=10, color = 'blue')), row = 1, col = 1)\n",
    "    elif pattern=='Evening Star':\n",
    "        evening_Star = df_tech[df_tech[f'Evening_Star_{company}']!=0]\n",
    "        fig.add_trace(go.Scatter(x=evening_Star['begin'], y=evening_Star[f'high_{company}'], mode='markers',\n",
    "                    name='evening_Star', marker=dict(symbol='circle', size=10, color = 'yellow')), row = 1, col = 1)\n",
    "    elif pattern==\"Hammer\":\n",
    "        hammer = df_tech[df_tech[f'Hammer_{company}']!=0]\n",
    "        fig.add_trace(go.Scatter(x=hammer['begin'], y=hammer[f'high_{company}'], mode='markers', name='Hammer',\n",
    "                         marker=dict(symbol='circle', size=10, color = 'orange')), row = 1, col = 1)\n",
    "    elif pattern==\"Morning Star\":\n",
    "        morningStar = df_tech[df_tech[f'Morning_Star_{company}']!=0]\n",
    "        fig.add_trace(go.Scatter(x=morningStar['begin'], y=morningStar[f'high_{company}'], mode='markers', name='morning_Star',\n",
    "                         marker=dict(symbol='circle', size=10, color = 'purple')), row = 1, col = 1)\n",
    "\n",
    "#Рисуем индикаторы -------------------------------------------------------------------------------------------------------\n",
    "\n",
    "    if indicator=='SMA':\n",
    "        #отрисовываем скользящую среднюю\n",
    "        sma = go.Scatter(x=df_tech['begin'],  y=df_tech[\"SMA_GAZP\"], name=\"SMA\", line=dict(color='blue'))\n",
    "        fig.add_trace(sma, row=1, col=1)\n",
    "    elif indicator==\"EMA\":\n",
    "        #отрисовываем экспоненциальную скользящую среднюю\n",
    "        ema = go.Scatter(x=df_tech['begin'], y=df_tech[\"EMA_GAZP\"], line=dict(color='yellow'), name = 'EMA')\n",
    "        fig.add_trace(ema, row=1, col=1)\n",
    "    elif indicator==\"BOLLINGER\":\n",
    "        #отрисовываем верхнюю полусу боллинджера\n",
    "        Bollinger_up = go.Scatter(x=df_tech['begin'], y=df_tech[f\"BOLLINGER_up_{company}\"], line=dict(color='blue'), name = 'BOLLINGER_up')\n",
    "        fig.add_trace(Bollinger_up, row=1, col=1)\n",
    "        #отрисовываем нижнюю полусу боллинджер\n",
    "        Bollinger_low = go.Scatter(x=df_tech['begin'], y=df_tech[f\"BOOLLINGER_low_{company}\"], line=dict(color='blue'), name = 'BOLLINGER_low')\n",
    "        fig.add_trace(Bollinger_low, row=1, col=1)\n",
    "        #отрисовываем серединную полусу боллинджер\n",
    "        Bollinger_mid = go.Scatter(x=df_tech['begin'], y=df_tech[f\"BOOLLINGER_mid_{company}\"], line=dict(color='orange'), name = 'BOLLINGER_mid')\n",
    "        fig.add_trace(Bollinger_mid, row=1, col=1)\n",
    "\n",
    "#Рисуем осцилляторы----------------------------------------------------------------------------------------\n",
    "\n",
    "    if oscillator==\"RSI\":\n",
    "        rsi = go.Scatter(x=df_tech['begin'], y=df_tech[f'RSI_{company}'], line=dict(color='purple'), name='RSI')\n",
    "        fig.add_trace(rsi, row = 2, col = 1)\n",
    "        fig.add_hline(y=20, row = 2, col = 1, line=dict(color=\"gray\"))\n",
    "        fig.add_hline(y=80, row = 2, col = 1, line=dict(color=\"gray\"))\n",
    "\n",
    "    elif oscillator==\"MACD\":\n",
    "        macd_usual = go.Scatter(x=df_tech['begin'], y=df_tech[f'MACD_{company}'], line=dict(color='blue'), name='MACD')\n",
    "        macd_signal = go.Scatter(x=df_tech['begin'], y=df_tech[f'MACDSIGNAL_{company}'], line=dict(color='orange'), name='Signal')\n",
    "        macdhist_bar = go.Bar(x=df_tech['begin'], y=df_tech[f'MACDHIST_{company}'], name='MACD Histogram', marker=dict(color='green'))\n",
    "        fig.add_trace(macd_usual, row = 2, col = 1)\n",
    "        fig.add_trace(macd_signal, row = 2, col = 1)\n",
    "        fig.add_trace(macdhist_bar, row = 2, col = 1)\n",
    "\n",
    "\n",
    "    elif oscillator==\"STOCH\":\n",
    "        slowk_trace = go.Scatter(x=df_tech['begin'], y=df_tech[f'STOCH_slowk{company}'], mode='lines', name='STOCH SlowK',line=dict(color='orange'))\n",
    "        slowd_trace = go.Scatter(x=df_tech['begin'], y=df_tech[f'STOCH_slowd{company}'], mode='lines', name='STOCH SlowD',line=dict(color='purple'))\n",
    "        fig.add_trace(slowk_trace, row=2, col=1)\n",
    "        fig.add_trace(slowd_trace, row=2, col=1)\n",
    "        fig.add_hline(y=20, row = 2, col = 1, line=dict(color=\"gray\", dash=\"dash\"))\n",
    "        fig.add_hline(y=80, row = 2, col = 1, line=dict(color=\"gray\", dash=\"dash\"))\n",
    "\n",
    "#------------------------------------------------------------------------\n",
    "    fig.update_layout(xaxis_rangeslider_visible=False) #дополнительные штуки для отрисовки\n",
    "    fig.update_layout(width=800, height=600, title=f\"{company} 2015 - 2023\", yaxis_title=f'{company} Stock')\n",
    "\n",
    "    filename=f'{company}.png'\n",
    "    fig.write_image(filename)\n",
    "    PATH=filename\n",
    "    im=pyimgur.Imgur(CLIENT_ID)\n",
    "    uploaded_image=im.upload_image(PATH, title=PATH)\n",
    "    return uploaded_image"
   ]
